# - Config file for the daily package
# It defines the following variables
#  DAILY_INCLUDE_DIRS	  - include directories for cppnetlib
#  DAILY_LIBRARIES    	  - libraries to link against (full path)
 
# Compute paths
get_filename_component(DAILY_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(DAILY_INCLUDE_DIRS "@DAILY_INCLUDE_DIRS@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(    NOT TARGET daily
   AND NOT DAILY_BINARY_DIR)
  include("${DAILY_CMAKE_DIR}/dailyTargets.cmake")
endif()
 
# These are IMPORTED targets created by dailyTargets.cmake
set(DAILY_LIBRARIES @DAILY_LIBRARIES@)

